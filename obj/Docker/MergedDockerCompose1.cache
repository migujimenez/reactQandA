services:
  qanda:
    build:
      context: /Users/migueljimenez/Projects/QandA
      dockerfile: QandA/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: QandA
      target: base
    container_name: QandA
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: qanda:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/QandA.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - /Users/migueljimenez/Projects/QandA/QandA:/app:rw
    - /Users/migueljimenez/vsdbg/vs2017u5:/remote_debugger:rw
    - /Users/migueljimenez/.aspnet/https:/root/.aspnet/https:ro
    - /Users/migueljimenez/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
    - /Users/migueljimenez/.nuget/packages:/root/.nuget/packages:ro
    - /Users/migueljimenez/Projects/QandA:/src:rw
version: '3.4'